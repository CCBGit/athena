<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">
       
	<bean id="DIRECT_RULE" class="com.ccb.testcenter.athena.dataprocess.entity.Rule">
		<property name="name" value="DIRECT_RULE" />
 		<property name="converter" ref="DIRECT_CONVERTER" />
	</bean>
	<bean id="CONSTANT_RULE" class="com.ccb.testcenter.athena.dataprocess.entity.Rule">
		<property name="name" value="CONSTANT_RULE" />
 		<property name="converter" ref="CONSTANT_CONVERTER" />
	</bean>
           
	<bean id="Dep_AcFrz_MdTp_Cd" class="com.ccb.testcenter.athena.dataprocess.entity.Rule">
		<property name="name" value="Dep_AcFrz_MdTp_Cd" />
 		<property name="converter" ref="Dep_AcFrz_MdTp_Cd_Converter" />
		<property name="params">
			<list>
				<value>TFT-FUNC</value>
			</list>
		</property>
	</bean>
	<bean id="Dep_Frz_TpCd" class="com.ccb.testcenter.athena.dataprocess.entity.Rule">
		<property name="name" value="Dep_Frz_TpCd" />
 		<property name="converter" ref="Dep_Frz_TpCd_Converter" />
		<property name="params">
			<list>
				<value>TFT-BUSN-TYPE</value>
			</list>
		</property>
	</bean>
</beans>